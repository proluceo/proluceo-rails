<% @accounts.each do |account| %>
  <li class="list-group-item relative cursor-default py-2 pl-3 text-gray-900 hover:bg-blue-600 hover:text-white active:bg-blue-600 active:text-white" role="option" data-autocomplete-value="<%= account.number %>" data-autocomplete-label="<%= account.number %> <%= account.label %>">
    <%= account %>
  </li>
<% end %>